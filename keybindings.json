// Platzieren Sie Ihre Tastenbindungen in dieser Datei, um die Standardwerte zu Ã¼berschreiben.
[
    { "key": "f1",                "command": "workbench.action.terminal.toggleTerminal" },
    { "key": "cmd+shift+t",                "command": "workbench.action.terminal.new" },
    { "key": "cmd+shift+j",                "command": "workbench.action.terminal.focus", "when": "!terminalFocus"},
    { "key": "cmd+shift+j",                "command": "workbench.action.terminal.focusNext", "when": "terminalFocus" }
,{
    "key": "shift+alt+t",
    "command": "workbench.action.tasks.runTask"
}]
